  <body>
    <%- include ("./partials/navbar.ejs") %>
    <main class="screen">
 <% if (allreserve.length > 0) {%>
      <table id="miyazaki">
        <thead>
        <tr>
          <th>User Name</th>
          <th>Email</th>
          <th>Ryad Name</th>
          <th>City</th>
          <th>Rooms</th>
          <th>At</th>
          <th>Price</th>
          <th>Action</th>
        </tr>
        <tbody>
    <% for (var i = 0; i < allreserve.length; i++) { %>
      <!-- <form action="/reservations/delete" method="POST"> -->
        <tr>
        <input type="hidden" class="id" name="reservId" value="<%= allreserve[i].reservationid %>">
        <td><%= allreserve[i].Susername %></td>
        <td><%= allreserve[i].Semail %></td>
        <td><%= allreserve[i].Sryadname %></td>
        <td><%= allreserve[i].Sryadcity %></td>
        <td><%= allreserve[i].Srooms %></td>
        <td><%= allreserve[i].Sryadaddress %></td>
        <td>MAD: <%= allreserve[i].Sprice %></td>
        <td><button id="resultButton" onClick="window.location.reload();"type="submit">Done</button></td>
        </tr>
      <!-- </form> -->
       <% } %>
        </tbody>
        </table>
    <%} else {%>
      <%- include ("./partials/none.ejs") %>
    <%}%>
    </main>
    <div style="height: 300px;"></div>
    <footer class="footer">
        <div>
          <div class="logorights">
